<h1>Lista de productos en tiempo real</h1>
<ul id="productsList">
  {{#each products}}
    <li id="product-{{this.id}}">
      <div>
        <h2>{{this.title}}</h2>
        <p>${{this.price}}</p>
        <img src={{this.thumbnails}} alt={{this.title}} width="100" height="100">
      </div>
    </li>
  {{/each}}
</ul>
<script src="/socket.io/socket.io.js"></script>
<script src="/js/realTimeProducts.js"></script>